{
   "content_scripts": [ {
      "js": [ "gmail_ext.js", "anywhere.js" ],
      "matches": [ "http://mail.google.com/*", "https://mail.google.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Trusted by over 1 million people, Todoist is the best to-do list. With our Gmail plugin you can conquer your inbox!",
   "icons": {
      "128": "todoist_128.png",
      "16": "todoist_16.png",
      "256": "todoist_256.png",
      "48": "todoist_48.png"
   },
   "manifest_version": 2,
   "name": "Todoist for Gmail",
   "permissions": [ "http://*.todoist.com/*", "https://*.todoist.com/*", "https://mail.google.com/*", "http://mail.google.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.7"
}
